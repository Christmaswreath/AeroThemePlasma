<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
    <mime-type type="application/vnd.microsoft.portable-executable">
        <comment>Windows or EFI program</comment>
        <glob-deleteall/>
        <glob pattern="*.cpl"/>
        <glob pattern="*.drv"/>
        <glob pattern="*.efi"/>
        <glob pattern="*.exe"/>
        <glob pattern="*.lib"/>
        <glob pattern="*.ocx"/>
        <glob pattern="*.scr"/>
        <glob pattern="*.sys"/>
    </mime-type>
</mime-info>
